{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Action":[
        "s3:GetObject","s3:PutObject","s3:ListBucket","s3:DeleteObject"
      ],
      "Resource":["arn:aws:s3:::eera-demo-te",
    "arn:aws:s3:::eera-demo-te/te-streaming-raw-layer/*",
    "arn:aws:s3:::eera-demo-te/te-parquet-table-layer/*"
      ]
    },
    {
      "Effect":"Allow",
      "Action":["secretsmanager:GetSecretValue"],
      "Resource":["arn:aws:secretsmanager:ap-south-2:<ACCOUNT_NUMBER>:secret:te-creds"]
    },
    {
      "Effect":"Allow",
      "Action":["glue:*","athena:*","catalog:*"],
      "Resource":["*"]
    },
    {
      "Effect":"Allow",
      "Action":["logs:CreateLogGroup","logs:CreateLogStream","logs:PutLogEvents"],
      "Resource":["*"]
    }
  ]
}