{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Action":[
        "kinesis:GetRecords",
        "kinesis:GetShardIterator",
        "kinesis:DescribeStream",
        "kinesis:ListStreams"
      ],
      "Resource":["arn:aws:kinesis:ap-south-2:<ACCOUNT_NUMBER>:stream/te-streaming-ds"]
    },
    {
      "Effect":"Allow",
      "Action":[
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource":[
        "arn:aws:s3:::eera-demo-te",
        "arn:aws:s3:::eera-demo-te/*"
      ]
    },
    {
      "Effect":"Allow",
      "Action":[
        "glue:StartJobRun",
        "glue:GetJobRun",
        "glue:GetJobRuns"
      ],
      "Resource":["arn:aws:glue:ap-south-2:<ACCOUNT_NUMBER>:job/te-parquet-job"]
    },
    {
      "Effect":"Allow",
      "Action":["secretsmanager:GetSecretValue"],
      "Resource":["arn:aws:secretsmanager:ap-south-2:<ACCOUNT_NUMBER>:secret:te-creds"]
    },
    {
      "Effect":"Allow",
      "Action":[
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": ["*"]
    }
  ]
}